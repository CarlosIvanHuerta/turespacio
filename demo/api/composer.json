{
    "name": "admin/api",
    "description": "Api para realizar todas las petici√≥n a la base de datos de tu respacio",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Admin\\Api\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Carlos Ivan HC"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "johnpbloch/wordpress-core-installer": true
        }
