    "minimum-stability": "dev",
    "prefer-stable": true,

    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "propel/propel": "~2.0@dev"
    },
    "autoload-dev": {
        "classmap": [
            "database", "database/generated-classes/"
        ]
    }
}
